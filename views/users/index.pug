extends ../common/common
block content
  h1 Users
  a.btn.btn-info(href="users/create") Create
  h3 List of user
  ul
   each user in users
    li
     span=user.name
     span &nbsp
     a(href="users/"+ user.id) View
     span &nbsp
     a(href="users/" + user.id + "/delete") Delete
     <br>
     p=user.gender
